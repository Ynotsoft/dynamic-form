{
  "name": "ynotsoft-dynamic-grid",
  "version": "1.0.35",
  "description": "DynamicGrid React component library",
  "type": "module",
  "main": "./dist/dynamic-grid.umd.cjs",
  "module": "./dist/dynamic-grid.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dynamic-grid.js",
      "require": "./dist/dynamic-grid.umd.cjs"
    },
    "./index.css": "./dist/ynotsoft-dynamic-grid.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ynotsoft/dynamic-form"
  },
  "files": [
    "dist",
    "index.css",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "dayjs": "^1.11.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-day-picker": "^8.0.0 || ^9.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "react-hot-toast": "^2.4.1",
    "react-select": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "lucide-react": "^0.554.0",
    "tailwind-merge": "^3.4.0"
  }
}
